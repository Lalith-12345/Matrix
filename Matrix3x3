#include <stdio.h>
main()
{
  int a[3][3],i,j;
  printf("Entre Matrix elements :\n");
  for(i=0;i<3;i++)
  {
    for(j=0;j<3;j++)
    {
      printf("Enter Elements in pockets[%d][%d]",i,j);
      scanf("%d",&a[i][j]);
    }
  }
  printf("\nMatrix is :\n");
  for(i=0;i<3;i++)
  {
    for(j=0;j<3;j++)
    {
      printf("%d\t",a[i][j]);
    }
    printf("\n");
  }
  return 0;
}
